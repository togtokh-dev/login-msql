{
  "name": "dev-login",
  "version": "1.0.3",
  "description": "",
  "scripts": {
    "production": "set NODE_ENV=production&& NODE_ENV=production node dist/server.js",
    "development": "set NODE_ENV=development&& NODE_ENV=development node dist/server.js",
    "stage": "set NODE_ENV=stage&& NODE_ENV=stage node dist/server.js",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "debug": "npm run build && npm run watch-debug",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect dist/server.js",
    "serve": "node dist/server.js",
    "start": "set NODE_ENV=production&& npm run serve",
    "test": "jest --forceExit --coverage --verbose",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "dev": "set NODE_ENV=development&& concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "dependencies": {
    "@google-cloud/logging": "^11.0.0",
    "@types/newrelic": "^9.4.0",
    "@types/winston": "^2.4.4",
    "auth-master": "^1.1.5",
    "axios": "^1.3.1",
    "axios-master": "^1.0.5",
    "body-parser": "^1.20.1",
    "compression": "1.7.4",
    "concurrently": "^7.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-and-time": "^2.4.2",
    "dotenv": "16.0.3",
    "errorhandler": "1.5.1",
    "express": "4.18.2",
    "express-fileupload": "^1.4.0",
    "ip": "^2.0.0",
    "lusca": "1.7.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.1.2",
    "newrelic": "^9.8.1",
    "response-master": "^0.0.3",
    "support-master": "^0.1.2",
    "uuid": "^9.0.0",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "8.21.0",
    "@types/express": "4.17.16",
    "@types/express-fileupload": "^1.4.1",
    "@types/ip": "^1.1.0",
    "@types/jest": "^29.4.0",
    "@types/lusca": "1.7.1",
    "@types/morgan": "^1.9.4",
    "@types/mysql": "^2.15.21",
    "@types/node": "^18.11",
    "@types/supertest": "2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.33.0",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "supertest": "6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
